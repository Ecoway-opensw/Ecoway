<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/title1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/istokweb_bold"
        android:text="ECOWAY"
        android:textSize="40px"
        android:textColor="#000000"
        android:layout_marginTop="83px"
        android:layout_marginLeft="43px"/>

    <ImageView
        android:layout_width="40px"
        android:layout_height="40px"
        android:src="@drawable/icon_leaf"
        android:layout_marginTop="93px"
        android:layout_marginLeft="225px"/>

    <TextView
        android:id="@+id/title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="마이페이지"
        android:textColor="#000000"
        android:textSize="15sp"
        android:fontFamily="@font/istokweb_bold"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="30dp"/>


    <Button
        android:id="@+id/login_button"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="80dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/click_button"
        android:text="로그인"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold" />

    <View
        android:layout_width="400dp"
        android:layout_marginLeft="6dp"
        android:layout_height="1dp"
        android:background="#000000"
        android:layout_marginTop="110dp"/>

    <TextView
        android:id="@+id/star_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="즐겨찾기 목록"
        android:textColor="#000000"
        android:textSize="15sp"
        android:fontFamily="@font/istokweb_bold"
        android:layout_marginTop="130dp"
        android:layout_marginLeft="30dp"/>

    <Button
        android:id="@+id/star1_button"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="160dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/click_button"
        android:text="환경 캠페인 브랜드"
        android:textSize="15dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"/>

    <Button
        android:id="@+id/star2_button"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/click_button"
        android:text="리사이클링 브랜드"
        android:textSize="15dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"/>

    <Button
        android:id="@+id/star3_button"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="240dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/click_button"
        android:text="환경 캠페인"
        android:textSize="15dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"/>

    <Button
        android:id="@+id/star4_button"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="280dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/click_button"
        android:text="리사이클링 방법"
        android:textSize="15dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"/>

    <View
        android:layout_width="400dp"
        android:layout_marginLeft="6dp"
        android:layout_height="1dp"
        android:background="#000000"
        android:layout_marginTop="340dp"/>

    <TextView
        android:id="@+id/modify_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="회원정보 수정"
        android:textColor="#000000"
        android:textSize="15sp"
        android:fontFamily="@font/istokweb_bold"
        android:layout_marginTop="360dp"
        android:layout_marginLeft="30dp"/>

    <Button
        android:id="@+id/unsubscribe_btn"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="390dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/click_button"
        android:text="회원 탈퇴"
        android:textSize="15dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"/>



    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="1349px"
        android:layout_marginTop="288px"></FrameLayout>



</FrameLayout>